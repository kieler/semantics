module test_procedure1:
procedure proc1(integer, boolean)(integer, integer, boolean);
output A : integer;
output B : boolean;

var i1 : integer,
    i2 : integer,
    i3 : integer,
    b1 : boolean,
    b2 : boolean in
  i1 := 1;
  i2 := 2;
  i3 := 5;
  b1 := false;
  b2 := true;
  call proc1(i3, b2)(i1, i2, b1);
  emit A(i3);
  emit B(b2)
end var

end module
