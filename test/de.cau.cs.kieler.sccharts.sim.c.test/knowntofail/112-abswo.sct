scchart ABSWO {
  input signal pure A;
  input signal pure B;
  output signal pure O;
  input signal pure W;
  input signal pure S;

  initial state ABO {

    region Main:

    initial state WaitAB {

      region WaitA:

      initial state wA
      --> dA with A / B;

      final state dA;

      region WaitB:

      initial state wB
      --> dB with B;

      final state dB;
    }
    >-> done / O;

    state done;
  }
  o-> ABO with S
  --> ABO with W;
}