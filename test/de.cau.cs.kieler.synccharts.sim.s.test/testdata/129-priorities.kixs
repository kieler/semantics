<?xml version="1.0" encoding="UTF-8"?>
<synccharts:Region xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:kexpressions="http://kieler.cs.cau.de/kexpressions/0.1.2" xmlns:synccharts="http://kieler.cs.cau.de/synccharts/0.2.4">
  <states id="transitions_and_signals" label="transitions_and_signals">
    <signals name="I1" isInput="true"/>
    <signals name="I2" isInput="true"/>
    <signals name="O1" isOutput="true"/>
    <signals name="O2" isOutput="true"/>
    <signals name="O3" isOutput="true"/>
    <regions>
      <states id="s1" label="s1" isInitial="true">
        <outgoingTransitions label="I1 and I2 / O1" priority="1" targetState="//@states.0/@regions.0/@states.1">
          <effects xsi:type="synccharts:Emission" signal="//@states.0/@signals.2"/>
          <trigger xsi:type="kexpressions:OperatorExpression" operator="and">
            <subExpressions xsi:type="kexpressions:ValuedObjectReference" valuedObject="//@states.0/@signals.0"/>
            <subExpressions xsi:type="kexpressions:ValuedObjectReference" valuedObject="//@states.0/@signals.1"/>
          </trigger>
        </outgoingTransitions>
        <outgoingTransitions label="I1 / O2" priority="2" targetState="//@states.0/@regions.0/@states.2">
          <effects xsi:type="synccharts:Emission" signal="//@states.0/@signals.3"/>
          <trigger xsi:type="kexpressions:ValuedObjectReference" valuedObject="//@states.0/@signals.0"/>
        </outgoingTransitions>
        <outgoingTransitions label="/ O3" priority="3" targetState="//@states.0/@regions.0/@states.3">
          <effects xsi:type="synccharts:Emission" signal="//@states.0/@signals.4"/>
        </outgoingTransitions>
      </states>
      <states id="s2" label="s2" incomingTransitions="//@states.0/@regions.0/@states.0/@outgoingTransitions.0"/>
      <states id="s3" label="s3" incomingTransitions="//@states.0/@regions.0/@states.0/@outgoingTransitions.1"/>
      <states id="s4" label="s4" incomingTransitions="//@states.0/@regions.0/@states.0/@outgoingTransitions.2"/>
    </regions>
  </states>
</synccharts:Region>
