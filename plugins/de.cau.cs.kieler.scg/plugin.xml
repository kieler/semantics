<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated scgdep -->
      <package
            uri="http://kieler.cs.cau.de/scgdep/0.1.0"
            class="de.cau.cs.kieler.scgdep.ScgdepPackage"
            genModel="model/scgdep.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated scg -->
      <package
            uri="http://kieler.cs.cau.de/scg/0.1.0"
            class="de.cau.cs.kieler.scg.ScgPackage"
            genModel="model/scg.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated scgbb -->
      <package
            uri="http://kieler.cs.cau.de/scgbb/0.1.0"
            class="de.cau.cs.kieler.scgbb.ScgbbPackage"
            genModel="model/scgbb.genmodel"/>
   </extension>
   <extension
         point="org.eclipse.emf.ecore.generated_package">
      <package
            class="de.cau.cs.kieler.scgsched.ScgschedPackage"
            genModel="model/scgsched.genmodel"
            uri="http://kieler.cs.cau.de/scgsched/0.1.0">
      </package>
   </extension>
   <extension
         point="de.cau.cs.kieler.kico.transformation">
      <productionTransformationClass
            class="de.cau.cs.kieler.scg.transformations.dependencies.DependencyTransformation">
      </productionTransformationClass>
      <productionTransformationClass
            class="de.cau.cs.kieler.scg.transformations.basicblocks.BasicBlockTransformation">
      </productionTransformationClass>
      <productionTransformationClass
            class="de.cau.cs.kieler.scg.transformations.guardExpressions.SimpleGuardExpressions">
      </productionTransformationClass>
      <productionTransformationClass
            class="de.cau.cs.kieler.scg.transformations.guards.SimpleGuardTransformation">
      </productionTransformationClass>
      <productionTransformationClass
            class="de.cau.cs.kieler.scg.transformations.schedulers.DCGuardScheduler">
      </productionTransformationClass>
      <productionTransformationClass
            class="de.cau.cs.kieler.scg.transformations.guards.FlatThreadsGuardTransformation">
      </productionTransformationClass>
      <productionTransformationClass
            class="de.cau.cs.kieler.scg.transformations.sequentializer.DCGuardSequentializer">
      </productionTransformationClass>
      <productionTransformationClass
            class="de.cau.cs.kieler.scg.transformations.sequentializer.SimpleGuardSequentializer">
      </productionTransformationClass>
      <productionTransformationClass
            class="de.cau.cs.kieler.scg.transformations.c.SCG2CTransformation">
      </productionTransformationClass>
      <productionTransformationClass
            class="de.cau.cs.kieler.scg.transformations.StructuralDepthJoinTransformation">
      </productionTransformationClass>
   </extension>
   <extension
         point="de.cau.cs.kieler.kico.feature">
      <featureClass
            class="de.cau.cs.kieler.scg.features.Dependency">
      </featureClass>
      <featureClass
            class="de.cau.cs.kieler.scg.features.BasicBlock">
      </featureClass>
      <featureClass
            class="de.cau.cs.kieler.scg.features.Guards">
      </featureClass>
      <featureClass
            class="de.cau.cs.kieler.scg.features.GuardExpressions">
      </featureClass>
      <featureClass
            class="de.cau.cs.kieler.scg.features.Scheduling">
      </featureClass>
      <featureClass
            class="de.cau.cs.kieler.scg.features.Sequentialize">
      </featureClass>
      <featureClass
            class="de.cau.cs.kieler.scg.features.Basic">
      </featureClass>
      <featureGroupClass
            class="de.cau.cs.kieler.scg.features.SCGraphGroup">
      </featureGroupClass>
      <featureClass 
            class="de.cau.cs.kieler.scg.features.SCPDG">
      </featureClass>
      <featureClass
            class="de.cau.cs.kieler.scg.features.DCSequentialize">
      </featureClass>
      <featureClass
            class="de.cau.cs.kieler.scg.features.C">
      </featureClass>
      <featureClass
            class="de.cau.cs.kieler.scg.features.StructuralDepthJoin">
      </featureClass>
   </extension>
   <extension
         point="de.cau.cs.kieler.kicool.processor">
      <Processor
            class="de.cau.cs.kieler.scg.processors.transformators.DependencyWrapper">
      </Processor>
      <Processor
            class="de.cau.cs.kieler.scg.processors.transformators.codegen.c.CCodeGenerator">
      </Processor>
      <Processor
            class="de.cau.cs.kieler.scg.processors.transformators.SequentializerWrapper">
      </Processor>
      <Processor
            class="de.cau.cs.kieler.scg.processors.transformators.SchedulerWrapper">
      </Processor>
      <Processor
            class="de.cau.cs.kieler.scg.processors.transformators.GuardsWrapper">
      </Processor>
      <Processor
            class="de.cau.cs.kieler.scg.processors.transformators.ExpressionsWrapper">
      </Processor>
      <Processor
            class="de.cau.cs.kieler.scg.processors.transformators.BasicBlocksWrapper">
      </Processor>
      <Processor
            class="de.cau.cs.kieler.scg.processors.analyzer.ControlflowValidator">
      </Processor>
      <Processor
            class="de.cau.cs.kieler.scg.processors.transformators.SimpleGuardScheduler">
      </Processor>
   </extension>
   <extension
         point="de.cau.cs.kieler.kicool.system">
      <TransformationSystem
            system="system/de.cau.cs.kieler.scg.netlist.kico">
      </TransformationSystem>
      <TransformationSystem
            system="system/de.cau.cs.kieler.sccharts.netlist.simple.kico">
      </TransformationSystem>
      <TransformationSystem
            system="system/de.cau.cs.kieler.sccharts.netlist.kico">
      </TransformationSystem>
      <TransformationSystem
            system="system/de.cau.cs.kieler.core.netlist.simple.kico">
      </TransformationSystem>
      <TransformationSystem
            system="system/de.cau.cs.kieler.sccharts.reference.core.netlist.simple.kico">
      </TransformationSystem>
      <TransformationSystem
            system="system/de.cau.cs.kieler.sccharts.dependency.kico">
      </TransformationSystem>
      <TransformationSystem
            system="system/de.cau.cs.kieler.scg.dependency.kico">
      </TransformationSystem>
   </extension>


</plugin>
