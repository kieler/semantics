<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.ui.ide.workbench"
            name="Synccharts">
         <property
               name="appName"
               value="Synccharts">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.xtend.typesystem.emf.checks">
      <metaModel
            nsURI="http://kieler.cs.cau.de/synccharts">
         <checkFile
               path="model/SyncchartsChecks.chk">
         </checkFile>
      </metaModel>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="de.cau.cs.kieler.synccharts.custom.RegisterCheckValidator">
      </startup>
   </extension>
   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://kieler.cs.cau.de/synccharts"
            class="de.cau.cs.kieler.synccharts.SyncchartsPackage"
            genModel="model/synccharts.genmodel"/>
   </extension>

</plugin>
