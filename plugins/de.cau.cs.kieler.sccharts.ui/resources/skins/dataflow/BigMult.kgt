knode BigMult {
	properties:
	org.eclipse.elk.portConstraints = FIXED_SIDE
	size: width = 15 height = 70
	kpolyline {
		styles: foreground = 150r
		points: left, bottom; left, top; right, top 15%; right, bottom 15%; left, bottom
	}
	kport in2 {
		size: width = 0 height = 0
		properties:
		org.eclipse.elk.port.side = WEST
	}
	kport in1 {
		size: width = 0 height = 0
		properties:
		org.eclipse.elk.layered.allowNonFlowPortsToSwitchSides = true
		org.eclipse.elk.port.side = SOUTH
	}
    kport in0 {
        size: width = 0 height = 0
        properties: 
        org.eclipse.elk.port.side = WEST
    }
	kport out {
		size: width = 0 height = 0
		pos: x = 31
		properties:
		org.eclipse.elk.port.borderOffset = -0.5
		org.eclipse.elk.port.side = EAST
	}	
}