<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated scg -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="de.cau.cs.kieler.scg.presentation.ScgModelWizardID"
            name="%_UI_ScgModelWizard_label"
            class="de.cau.cs.kieler.scg.presentation.ScgModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ScgModelFile.gif">
         <description>%_UI_ScgModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>



   <extension point="org.eclipse.ui.editors">
      <!-- @generated scg -->
      <editor
            id="de.cau.cs.kieler.scg.presentation.ScgEditorID"
            name="%_UI_ScgEditor_label"
            icon="icons/full/obj16/ScgModelFile.gif"
            extensions="scg"
            class="de.cau.cs.kieler.scg.presentation.ScgEditor"
            contributorClass="de.cau.cs.kieler.scg.presentation.ScgActionBarContributor">
      </editor>
   </extension>

   <extension
         point="de.cau.cs.kieler.kico.ui.transformation">
      <editor
            editor="de.cau.cs.kieler.scg.presentation.ScgEditorID"
            transformations="SCGOPT_NODES,SCGRAPH, SCG, SCGDEP, SCGBB, SCGBBSCPLUS, SCGSCHEDSIMPLE, SCGSCHEDDELAYAWARE, SCGSCHEDTHREADAWARE, SCGSEQHYBRID, SCGSEQSIMPLE, SCGSEQGROUP, SCGBBGROUP, SCGSCHEDGROUP,CODEGENERATION, SCG2S, SCG2C">
      </editor>
   </extension>

</plugin>
