           ePackageName="esterel"
           editor_id="de.cau.cs.kieler.esterel.scest.SCEst"
           extensionName="scest"
           isXMI="false"
           predicate="de.cau.cs.kieler.esterel.scest.SCEstPredicate">
     </ExtensionInformation>
  </extension>
  <extension
        point="de.cau.cs.kieler.kicool.processor">
     <Processor
           class="de.cau.cs.kieler.esterel.processors.transformators.ssa.SCEstToSSCSCLTransformation">
     </Processor>
     <Processor
           class="de.cau.cs.kieler.esterel.processors.transformators.ssa.SSASCL2SSAEsterel">
     </Processor>
     <Processor
           class="de.cau.cs.kieler.esterel.processors.transformators.ssa.SSCEsterelReconstruction">
