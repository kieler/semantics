                        INIT := false;
                        K := Ky;
                        T_K := T_Ky;
                        MIN_CORR_K := MIN_CORR_Ky
                      end if;
                      TIME := 0
                    end if;
                    ANG_CORR_VALUE := ANG_VALUE + ZVOLM / 256
