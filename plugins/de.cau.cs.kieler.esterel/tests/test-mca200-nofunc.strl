                    INIT := true;
                    TIME := 0;
                    TIME_Kt := 0
                  end if
                end present;
                present ANG then
                  ANG_VALUE := ?ANG;
                  if SPEED_VALUE > MAX_SPEED_VALUE then
                    TIME_Kt := TIME_Kt + 1;
                    if TIME_Kt = T_Kt then
                      ZVOL := ZVOL_Kt + K * (ANG_VALUE - ZVOL_Kt / 256);
