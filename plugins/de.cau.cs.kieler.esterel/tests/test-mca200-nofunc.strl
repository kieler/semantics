                                                            V2 := VAHR_20
                                                          else
                                                            if SPEED_VALUE < SPEED_210_KM then
                                                              V1 := VAMR_21;
                                                              V2 := VAHR_21
                                                            else
                                                              if SPEED_VALUE < SPEED_220_KM then
