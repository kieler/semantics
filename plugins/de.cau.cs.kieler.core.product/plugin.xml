         </property>
         <property
               name="aboutImage"
               value="images/about.png">
         </property>
         <property
               name="appName"
               value="KIELER">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="introTitle"
               value="KIELER - Kiel Integrated Environment for Layout Eclipse Rich Client">
         </property>
         <property
               name="startupForegroundColor"
               value="000000">
         </property>
         <property
               name="startupProgressRect"
               value="5,275,445,15">
         </property>
         <property
               name="version"
               value="0.6.0">
         </property>
         <property
               name="windowImages"
               value="icons/kiel_16.gif,icons/kiel_32.gif,icons/kiel_48.gif,icons/kiel_64.gif,icons/kiel_128.gif">
         </property>
         <property
               name="introBrandingImage"
               value="product:icons/kiel_64.gif">
         </property>
         <property
               name="introBrandingImageText"
               value="KIELER by Christian-Albrechts-UniversitÃ¤t zu Kiel">
         </property>
         <property
               name="startupMessageRect"
               value="7,252,445,20">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.splashHandlers">
      <splashHandler
            class="de.cau.cs.kieler.core.product.splash.KielerSplashHandler"
            id="de.cau.cs.kieler.core.product.splash.extensible">
      </splashHandler>
      <splashHandlerProductBinding
            productId="de.cau.cs.kieler.core.product.product"
            splashId="de.cau.cs.kieler.core.product.splash.extensible">
      </splashHandlerProductBinding>
   </extension>

</plugin>
