      <children xsi:type="org.openarchitectureware.xtext:Group" cardinality="OPTIONAL">
        <children xsi:type="org.openarchitectureware.xtext:Keyword" value="/"/>
        <children xsi:type="org.openarchitectureware.xtext:Alternatives" cardinality="ANY">
          <alternatives xsi:type="org.openarchitectureware.xtext:Assignment" feature="effects" operator="ADD">
