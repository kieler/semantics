    <content xsi:type="org.openarchitectureware.xtext:Group">
      <children xsi:type="org.openarchitectureware.xtext:Keyword" value="("/>
      <children xsi:type="org.openarchitectureware.xtext:Assignment" cardinality="OPTIONAL" feature="subExpressions" operator="ADD">
