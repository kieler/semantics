      <children xsi:type="org.openarchitectureware.xtext:Assignment" feature="operator" operator="ASSIGN">
        <token xsi:type="org.openarchitectureware.xtext:RuleName" name="Operator"/>
      </children>
      <children xsi:type="org.openarchitectureware.xtext:Assignment" feature="subExpressions" operator="ADD">
        <token xsi:type="org.openarchitectureware.xtext:RuleName" name="Expression"/>
      </children>
      <children xsi:type="org.openarchitectureware.xtext:Keyword" value=")"/>
    </content>
  </rules>
  <rules xsi:type="org.openarchitectureware.xtext:TypeRule" name="Emission">
