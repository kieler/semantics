    <content xsi:type="org.openarchitectureware.xtext:Group">
      <children xsi:type="org.openarchitectureware.xtext:Assignment" cardinality="OPTIONAL" feature="trigger" operator="ASSIGN">
        <token xsi:type="org.openarchitectureware.xtext:RuleName" name="Expression"/>
